// Colors
cDark = #2c2c2c
cLight = #efefef

// Fonts
fMain = Raleway

// Mixins
user-select()
  -webkit-touch-callout: none;
  -webkit-tap-highlight-color: transparent;
  -webkit-user-select: none; /* Safari 3.1+ */
  -moz-user-select: none; /* Firefox 2+ */
  -ms-user-select: none; /* IE 10+ */
  user-select: none; /* Standard syntax */
 